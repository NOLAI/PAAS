{
  "name": "@nolai/paas-client",
  "version": "0.1.3",
  "description": "A client library for interacting with PaaS",
  "type": "module",
  "main": "dist/paas-client.js",
  "exports": {
    "browser": "./dist/paas-client.browser.js",
    "node": "./dist/paas-client.js",
    "default": "./dist/paas-client.js"
  },
  "module": "dist/paas-client.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/paas-client.browser.umd.js",
    "dist/paas-client.browser.esm.js",
    "dist/paas-client.node.js",
    "dist/index.d.ts",
    "README.md"
