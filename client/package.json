    "@rollup/plugin-typescript": "^12.1.1",
    "@rollup/plugin-wasm": "^6.2.2",
    "@types/jest": "^29.5.14",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
